{
  "name": "newdocs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "yarn run apidocs:rebuild && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "run-s lint:*",
    "lint:text": "textlint docs/",
    "lint:markdown": "markdownlint docs/*.md --config .markdownlint.json",
    "apidocs:rebuild": "docusaurus clean-api-docs all && docusaurus gen-api-docs all && ts-node hack/fix-api-sidebar.ts"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.0",
    "@docusaurus/preset-classic": "3.8.0",
    "@docusaurus/theme-mermaid": "3.8.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "docusaurus-plugin-openapi-docs": "^4.5.1",
    "docusaurus-theme-openapi-docs": "^4.5.1",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "textlint-rule-terminology": "4",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.0",
    "@docusaurus/tsconfig": "3.8.0",
    "@docusaurus/types": "3.8.0",
    "@elasticpath/textlint-rule-no-dead-relative-link": "^1.1.1",
    "markdownlint": "^0.17.0",
    "markdownlint-cli": "^0.19.0",
    "npm-run-all": "^4.0.0",
    "textlint": "^12.0.0",
    "textlint-rule-no-dead-link": "^5.0.0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
